{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "sh",
			"args": [
				"tools/bump_version.sh",
				"pop_pop",
			],
			"group": "none",
			"label": "Bump version: pop_pop"
		},
		{
			"type": "flutter",
			"command": "flutter",
			"options": {
				"cwd": "packages/samples/pop_pop_sample"
			},
			"args": [
				"pub",
				"run",
				"build_runner",
				"build",
				"--delete-conflicting-outputs"
			],
			"problemMatcher": [
				"$dart-build_runner"
			],
			"group": "build",
			"label": "pop_pop_sample: build_runner"
		},
	]
}